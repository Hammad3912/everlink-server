{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users",
      "dest": "/app.js"
    },
    {
      "src": "/api/links",
      "dest": "/app.js"
    },
    {
      "src": "/api/pages",
      "dest": "/app.js"
    },
    {
      "src": "/api/4everland",
      "dest": "/app.js"
    },
    {
      "src": "/api/deploymentHistory",
      "dest": "/app.js"
    },
    {
      "src": "/api/crossmint",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "05 16 * * *"
    }
  ]
}
